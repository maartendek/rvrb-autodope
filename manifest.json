{
  "manifest_version": 3,
  "name": "RVRB AutoDope",
  "description": "Enable autodope on RVRB.",
  "version": "1.0",
  "icons": {
    "16": "images/rvrb-ad.png",
    "32": "images/rvrb-ad.png",
    "48": "images/rvrb-ad.png",
    "128": "images/rvrb-ad.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": [
    "scripting",
    "activeTab",
    "declarativeContent"
  ]
}